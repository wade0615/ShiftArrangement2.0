=\"authCode\"\n        tag=\"form\"\n        @submit.prevent=\"submit()\"\n      >\n        <div class=\"mb-8\">\n          <ValidationProvider ref=\"email\" :rules=\"rules.otpCode\">\n            <p class=\"mb-2\">驗證碼*</p>\n            <v-otp-input\n              v-model=\"validationForm.otpCode\"\n              length=\"6\"\n              type=\"number\"\n            ></v-otp-input>\n            <span class=\"text-sm leading-normal\"\n              >已將驗證碼寄送至 {{ registerData.email }}</span\n            >\n          </ValidationProvider>\n        </div>\n        <div class=\"text-center mb-8\">\n          <StyledBtn\n            text=\"下一步\"\n            large\n            text-color=\"#fff\"\n            bg-color=\"#46B964\"\n            min-width=\"160\"\n            type=\"submit\"\n            :disabled=\"invalid\"\n          ></StyledBtn>\n        </div>\n        <div class=\"text-center mb-8\">\n          <p>還沒收到驗證碼嗎？</p>\n          <p v-if=\"countTime > 0\" @click=\"resendOtp()\">\n            ({{ countTime }})秒後可重新寄送\n          </p>\n          <v-btn\n            v-else\n            text\n            color=\"#007AFF\"\n            large\n            min-width\n            @click=\"resendOtp()\"\n            >重新寄送驗證碼</v-btn\n          >\n        </div>\n        <div class=\"text-center mb-8\">\n          <v-btn text color=\"#007AFF\" large min-width @click=\"handleOtp()\"\n            >修改 email 信箱</v-btn\n          >\n        </div>\n      </ValidationObserver>\n    </section>\n    <StyledDialog\n      :value=\"value\"\n      title=\"驗證信已重新寄送\"\n      :text=\"`已將驗證信寄送至 ${registerData.email}`\"\n      @handleDialog=\"handleDialog\"\n    />\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    registerData: {\n      type: Object,\n      default: () => {\n        return {\n          email: '',\n          password: '',\n        }\n      },\n    },\n  },\n  data: () => {\n    return {\n      value: false,\n      email: 'test@mail.com',\n      validationForm: {\n        otpCode: '',\n      },\n      rules: {\n        otpCode: 'required|min:6',\n      },\n      countTime: 60,\n    }\n  },\n  computed: {},\n  watch: {},\n  mounted() {\n    this.timmer()\n  },\n  beforeMount() {},\n  updated() {},\n  methods: {\n    async submit() {\n      /** 取得驗證是否通過，通過為 true */\n      const isValid = await this.$refs.authCode.validate()\n      if (isValid) {\n        this.$router.push({ name: 'main-registerDone' })\n      }\n    },\n    handleOtp() {\n      this.$emit('handle_otp')\n    },\n    /** 重新寄送驗證碼 */\n    resendOtp() {\n      console.log('重新寄送驗證碼')\n      this.value = !this.value\n    },\n    /** 重新寄送驗證碼倒計時 */\n    timmer() {\n      const nIntervId = setInterval(() => {\n        this.countTime = this.countTime - 1\n      }, 1000)\n      setTimeout(() => {\n        clearInterval(nIntervId)\n      }, 60000)\n    },\n    handleDialog() {\n      this.value = !this.value\n    },\n  },\n}\n</script>\n\n<style lang=\"sass\" scoped>\nspan\n  color: #7E7E7E\n</style>\n","/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/components/register/index.js",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/registerDone.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/plugins/styledCom/StyledDialog.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/plugins/vuetify.js",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/store/addstore.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/store/StoreList.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/store/AddStore.vue",["137","138"],[],"<template>\n  <main id=\"add-store\" class=\"px-4 py-8\" data-app>\n    <ValidationObserver\n      v-slot=\"{ invalid }\"\n      ref=\"addStore\"\n      tag=\"form\"\n      @submit.prevent=\"submit()\"\n    >\n      <h2 class=\"mb-8 text-center\">新增店面資訊</h2>\n      <!-- 第一步 -->\n      <section v-if=\"pageOne\">\n        <ValidationProvider ref=\"storeName\" :rules=\"rules.storeName\">\n          <p class=\"mb-2\">店面名稱*</p>\n          <StyledInput\n            v-model=\"validationForm.storeName\"\n            placeholder=\"請輸入店面名稱\"\n            cssclass=\"mb-2\"\n            required\n          />\n        </ValidationProvider>\n        <ValidationProvider ref=\"publicHoliday\" :rules=\"rules.publicHoliday\">\n          <p class=\"mb-2\">店面公休日*</p>\n          <StyledInput\n            v-model=\"validationForm.publicHoliday\"\n            type=\"chipsSelect\"\n            placeholder=\"請輸入店面公休日\"\n            :select-options=\"dayOptions\"\n            cssclass=\"mb-2\"\n            required\n          />\n        </ValidationProvider>\n        <ValidationProvider\n          ref=\"separateFrontAndBack\"\n          :rules=\"rules.separateFrontAndBack\"\n        >\n          <section class=\"flex justify-between items-center\">\n            <p class=\"mb-2\">店內有無分內外場*</p>\n            <StyledInput\n              v-model=\"validationForm.separateFrontAndBack\"\n              type=\"radio\"\n              :radio-options=\"radioOptions\"\n              radio-direction=\"row\"\n              cssclass=\"mb-2\"\n              required\n            />\n          </section>\n        </ValidationProvider>\n        <div class=\"text-center mb-8\">\n          <StyledBtn\n            text=\"下一步\"\n            large\n            text-color=\"#fff\"\n            bg-color=\"#46B964\"\n            min-width=\"160\"\n            type=\"submit\"\n            :disabled=\"invalid\"\n          ></StyledBtn>\n        </div>\n      </section>\n      <!-- 第二步 -->\n      <section v-else>\n        <v-card elevation=\"2\" class=\"p-3\">\n          <p class=\"mb-2\">設定店面班別</p>\n          <v-tabs v-model=\"tab\" fixed-tabs>\n            <v-tabs-slider color=\"#46B964\"></v-tabs-slider>\n            <v-tab v-for=\"weekDay in weekDayOptions\" :key=\"weekDay.value\">\n              {{ weekDay.label }}\n            </v-tab>\n          </v-tabs>\n          <v-tabs-items v-model=\"tab\">\n            <v-tab-item v-for=\"weekDay in weekDayOptions\" :key=\"weekDay.value\">\n              <v-card flat>\n                <div\n                  v-if=\"validationForm.publicHoliday.includes(weekDay.value)\"\n                  class=\"py-2.5 text-center\"\n                >\n                  您已將本日設為公休日\n                </div>\n                <div\n                  v-if=\"\n                    weekDay.shifts.length === 0 &&\n                    !validationForm.publicHoliday.includes(weekDay.value)\n                  \"\n                  class=\"py-2.5 text-center\"\n                >\n                  <v-card-text class=\"bg-light-gray\" @click=\"addShift()\">\n                    您尚未添加任何班別\n                  </v-card-text>\n                  <StyledBtn\n                    text=\"新增班別\"\n                    text-color=\"#46B964\"\n                    bg-color=\"#fff\"\n                    min-width=\"160\"\n                    type=\"button\"\n                    elevation=\"0\"\n                    :disabled=\"invalid\"\n                    @click=\"addShift()\"\n                  ></StyledBtn>\n                </div>\n              </v-card>\n            </v-tab-item>\n          </v-tabs-items>\n        </v-card>\n        <StyledDialog\n          :value=\"activeDialog\"\n          title=\"新增班別\"\n          confirm-text=\"新增\"\n          @handleDialog=\"handleDialog\"\n        >\n          <AddStoreShift @onchange=\"addNewShift\" />\n        </StyledDialog>\n      </section>\n    </ValidationObserver>\n  </main>\n</template>\n\n<script>\nimport AddStoreShift from '@/pages/main/store/AddStoreShift'\n\nexport default {\n  name: 'AddStore',\n  components: { AddStoreShift },\n  layout: 'main',\n  props: [],\n  data: () => {\n    return {\n      validationForm: {\n        storeName: '',\n        publicHoliday: [],\n        separateFrontAndBack: 'false',\n      },\n      rules: {\n        storeName: 'required|max:30',\n        publicHoliday: 'required',\n        separateFrontAndBack: 'required',\n      },\n      dayOptions: [\n        {\n          label: '無',\n          value: 'none',\n        },\n        {\n          label: '星期一',\n          value: 'Mon',\n        },\n        {\n          label: '星期二',\n          value: 'Tue',\n        },\n        {\n          label: '星期三',\n          value: 'Wed',\n        },\n      ],\n      radioOptions: [\n        {\n          label: '有',\n          value: 'true',\n        },\n        {\n          label: '沒有',\n          value: 'false',\n        },\n      ],\n      weekDayOptions: [\n        { label: '一', value: 'Mon', shifts: [] },\n        { label: '二', value: 'Tue', shifts: [] },\n        { label: '三', value: 'Wed', shifts: [] },\n        { label: '四', value: 'Thu', shifts: [] },\n        { label: '五', value: 'Fri', shifts: [] },\n        { label: '六', value: 'Sat', shifts: [] },\n        { label: '日', value: 'Sun', shifts: [] },\n      ],\n      pageOne: false,\n      tab: null,\n      activeDialog: false,\n    }\n  },\n  computed: {},\n  watch: {},\n  mounted() {},\n  beforeMount() {},\n  updated() {},\n  methods: {\n    async submit() {\n      /** 取得驗證是否通過，通過為 true */\n      const isValid = await this.$refs.addStore.validate()\n      if (isValid) {\n        console.log('通過驗證', this.validationForm)\n        this.pageOne = false\n      }\n    },\n    addShift() {\n      this.handleDialog()\n    },\n    handleDialog() {\n      this.activeDialog = !this.activeDialog\n    },\n    addNewShift(values) {\n      console.log('addNewShift', values)\n      // TODO 推進外面的 form 裡\n    },\n  },\n}\n</script>\n\n<style lang=\"sass\" scoped>\n#add-store\n  h2\n    flex: 0 1 auto\n    font-weight: 500\n    font-size: 1.25rem\n    line-height: 1.5\n  .v-tab\n    min-width: 40px\n    padding: 0 0.25rem\n</style>\n","/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/clie