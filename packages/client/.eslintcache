ckbox\"\n          :checkbox-options=\"checkboxOptions\"\n        />\n        <p class=\"mb-2\">複製此班別至其他工作日</p>\n        <StyledInput\n          v-model=\"validationForm.copyShift\"\n          type=\"checkbox\"\n          :checkbox-options=\"copyShiftOptions\"\n        />\n        <StyledBtn\n          text-color=\"#fff\"\n          bg-color=\"#46B964\"\n          min-width=\"160\"\n          type=\"submit\"\n          elevation=\"0\"\n          >新增</StyledBtn\n        >\n      </ValidationProvider>\n    </ValidationObserver>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'AddStoreShift',\n  layout: 'default',\n  props: {\n    storeInfo: {\n      type: Object,\n      default: () => {\n        return {\n          storeName: '',\n          publicHoliday: [],\n          separateFrontAndBack: false,\n          weekDayShifts: [],\n        }\n      },\n    },\n    currentDay: {\n      type: Number,\n      default: 0,\n    },\n  },\n  data: () => {\n    return {\n      validationForm: {\n        shiftName: '',\n        startTime: null,\n        endTime: null,\n        configuration: [],\n        copyShift: [],\n      },\n      rules: {\n        shiftName: 'required|max:30',\n        startTime: 'required',\n        endTime: 'required',\n      },\n      checkboxOptions: [\n        {\n          label: '外場',\n          value: 'front',\n        },\n        {\n          label: '內場',\n          value: 'back',\n        },\n      ],\n      copyShiftOptions: [\n        {\n          label: '週一',\n          value: 0,\n        },\n        {\n          label: '週二',\n          value: 1,\n        },\n        {\n          label: '週三',\n          value: 2,\n        },\n        {\n          label: '週四',\n          value: 3,\n        },\n        {\n          label: '週五',\n          value: 4,\n        },\n        {\n          label: '週六',\n          value: 5,\n        },\n        {\n          label: '週日',\n          value: 6,\n        },\n      ],\n    }\n  },\n  computed: {},\n  watch: {\n    validationForm: {\n      handler(val) {\n        this.$emit('onchange', val)\n      },\n      deep: true,\n    },\n  },\n  mounted() {},\n  beforeMount() {\n    this.setCopyShiftOptions()\n  },\n  updated() {},\n  methods: {\n    async submit() {\n      /** 取得驗證是否通過，通過為 true */\n      const isValid = await this.$refs.addstoreShift.validate()\n      if (isValid) {\n        this.$emit('onSubmit', this.validationForm)\n        // window.requestAnimationFrame(() => {\n        //   this.$refs.addStore.reset()\n        //   console.log('close')\n        // })\n      }\n    },\n    setCopyShiftOptions() {\n      console.log('setCopyShiftOptions', this.storeInfo.publicHoliday)\n      const copyShiftOptions = JSON.parse(JSON.stringify(this.copyShiftOptions))\n      const currentCopyDays = copyShiftOptions.filter(\n        (option, index) =>\n          !this.storeInfo.publicHoliday.includes(index) &&\n          index !== this.currentDay\n      )\n      this.copyShiftOptions = currentCopyDays\n    },\n  },\n}\n</script>\n\n<style lang=\"sass\" scoped>\n#add-store-shift\n  position: relative\n</style>\n","/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/plugins/styledCom/TimePicker.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/dashboard/index.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/store/edit/StoreInfo.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/store/edit/RoutineShifts.vue",[],[],{"ruleId":"156","replacedBy":"157"},{"ruleId":"158","replacedBy":"159"},{"ruleId":"160","replacedBy":"161"},{"ruleId":"162","replacedBy":"163"},{"ruleId":"164","severity":1,"message":"165","line":111,"column":7,"nodeType":"166","messageId":"167","endLine":111,"endColumn":18},{"ruleId":"164","severity":1,"message":"165","line":279,"column":9,"nodeType":"166","messageId":"167","endLine":279,"endColumn":20},{"ruleId":"164","severity":1,"message":"165","