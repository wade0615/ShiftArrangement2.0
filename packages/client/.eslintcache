          <span class=\"text-sm leading-normal\"\n              >已將驗證碼寄送至 {{ registerData.email }}</span\n            >\n          </ValidationProvider>\n        </div>\n        <div class=\"text-center mb-8\">\n          <StyledBtn\n            large\n            text-color=\"#fff\"\n            bg-color=\"#46B964\"\n            min-width=\"160\"\n            type=\"submit\"\n            :disabled=\"invalid\"\n            >下一步</StyledBtn\n          >\n        </div>\n        <div class=\"text-center mb-8\">\n          <p>還沒收到驗證碼嗎？</p>\n          <p v-if=\"countTime > 0\" @click=\"resendOtp()\">\n            ({{ countTime }})秒後可重新寄送\n          </p>\n          <v-btn\n            v-else\n            text\n            color=\"#007AFF\"\n            large\n            min-width\n            @click=\"resendOtp()\"\n            >重新寄送驗證碼</v-btn\n          >\n        </div>\n        <div class=\"text-center mb-8\">\n          <v-btn text color=\"#007AFF\" large min-width @click=\"handleOtp()\"\n            >修改 email 信箱</v-btn\n          >\n        </div>\n      </ValidationObserver>\n    </section>\n    <StyledDialog\n      :value=\"value\"\n      title=\"驗證信已重新寄送\"\n      :text=\"`已將驗證信寄送至 ${registerData.email}`\"\n      @handleDialog=\"handleDialog\"\n    />\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    registerData: {\n      type: Object,\n      default: () => {\n        return {\n          email: '',\n          password: '',\n        }\n      },\n    },\n  },\n  data: () => {\n    return {\n      value: false,\n      email: 'test@mail.com',\n      validationForm: {\n        otpCode: '',\n      },\n      rules: {\n        otpCode: 'required|min:6',\n      },\n      countTime: 60,\n    }\n  },\n  computed: {},\n  watch: {},\n  mounted() {\n    this.timmer()\n  },\n  beforeMount() {},\n  updated() {},\n  methods: {\n    async submit() {\n      /** 取得驗證是否通過，通過為 true */\n      const isValid = await this.$refs.authCode.validate()\n      if (isValid) {\n        this.$router.push({ name: 'main-registerDone' })\n      }\n    },\n    handleOtp() {\n      this.$emit('handle_otp')\n    },\n    /** 重新寄送驗證碼 */\n    resendOtp() {\n      console.log('重新寄送驗證碼')\n      this.value = !this.value\n    },\n    /** 重新寄送驗證碼倒計時 */\n    timmer() {\n      const nIntervId = setInterval(() => {\n        this.countTime = this.countTime - 1\n      }, 1000)\n      setTimeout(() => {\n        clearInterval(nIntervId)\n      }, 60000)\n    },\n    handleDialog() {\n      this.value = !this.value\n    },\n  },\n}\n</script>\n\n<style lang=\"sass\" scoped>\nspan\n  color: #7E7E7E\n</style>\n","/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/components/register/index.js",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/registerDone.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/plugins/styledCom/StyledDialog.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/plugins/vuetify.js",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/store/addstore.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/store/StoreList.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/store/AddStore.vue",["194"],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/store/StoreDetail.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/components/Stepper.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/store/AddStoreShift.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/plugins/styledCom/TimePicker.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/dashboard/index.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/store/edit/StoreInfo.vue",["195"],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client