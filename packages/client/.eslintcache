)\n      const copyShiftOptions = JSON.parse(JSON.stringify(this.copyShiftOptions))\n      const currentCopyDays = copyShiftOptions.filter(\n        (option, index) =>\n          !this.storeInfo.publicHoliday.includes(index) &&\n          index !== this.currentDay\n      )\n      this.copyShiftOptions = currentCopyDays\n    },\n  },\n}\n</script>\n\n<style lang=\"sass\" scoped>\n#add-store-shift\n  position: relative\n</style>\n","/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/plugins/styledCom/TimePicker.vue",[],[],"/Users/shu-weiwu/projects/ShiftArrangement2.0/packages/client/pages/main/dashboard/index.vue",[],[],{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","replacedBy":"148"},{"ruleId":"149","replacedBy":"150"},{"ruleId":"151","replacedBy":"152"},{"ruleId":"153","severity":1,"message":"154","line":111,"column":7,"nodeType":"155","messageId":"156","endLine":111,"endColumn":18},{"ruleId":"153","severity":1,"message":"154","line":279,"column":9,"nodeType":"155","messageId":"156","endLine":279,"endColumn":20},{"ruleId":"153","severity":1,"message":"154","line":160,"column":7,"nodeType":"155","messageId":"156","endLine":160,"endColumn":18},"unicorn/no-array-instanceof",["157"],"unicorn/prefer-exponentiation-operator",["158"],"unicorn/prefer-starts-ends-with",["159"],"unicorn/prefer-text-content",["160"],"no-console","Unexpected console statement.","MemberExpression","unexpected","unicorn/no-instanceof-array","prefer-exponentiation-operator","unicorn/prefer-string-starts-ends-with","unicorn/prefer-dom-node-text-content"]